<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="geodatahubback.mapper.UserDataMapper">

    <resultMap id="BaseResultMap" type="geodatahubback.entity.UserData">
            <result property="id" column="id" jdbcType="BIGINT"/>
            <result property="name" column="real_name" jdbcType="VARCHAR"/>
            <result property="description" column="description" jdbcType="VARCHAR"/>
            <result property="storeName" column="store_name" jdbcType="VARCHAR"/>
            <result property="webAddress" column="file_web_address" jdbcType="VARCHAR"/>
            <result property="realPath" column="file_path" jdbcType="VARCHAR"/>
            <result property="metaId" column="meta_id" jdbcType="VARCHAR"/>
            <result property="deleted" column="deleted" jdbcType="INTEGER"/>
            <result property="version" column="version" jdbcType="INTEGER"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,name,description,
        store_name,web_address,real_path,
        meta_id,deleted,version,
        create_time,update_time
    </sql>
</mapper>
